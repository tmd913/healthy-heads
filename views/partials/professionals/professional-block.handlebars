<li class="result-item" data-profID={{id}}>
    <div class="row">
        <div class="col s2">
            <img class="prof-pic" src="{{prof_photo}}" alt="Profile Picture">
        </div>
        <div id="main-info" class="col s7">
            <p class="name-title">
                <span class="name">
                    {{prof_first_name}} {{prof_last_name}}
                </span>
                <span>-</span>
                <span class="title">
                    {{prof_title}}
                </span>
            </p>
            <p class="specialties">Specialties: {{#if prof_specialty1}}
                <span class="specialty1">
                    {{prof_specialty1}}
                </span>
                {{/if}} {{#if prof_specialty2}}
                <span>|</span>
                <span class="specialty2">
                    {{prof_specialty2}}
                </span>
                {{/if}} {{#if prof_specialty3}}
                <span>|</span>
                <span class="specialty3">
                    {{prof_specialty3}}
                </span>
                {{/if}}
            </p>
            <p class="gender">Gender: {{prof_gender}}</p>
            <p class="years">Years Practiced: {{prof_years}}</p>
            <p class="languages">Languages: {{#if prof_language1}}
                <span class="language1">
                    {{prof_language1}}
                </span>
                {{/if}} {{#if prof_language2}}
                <span>|</span>
                <span class="language2">
                    {{prof_language2}}
                </span>
                {{/if}} {{#if prof_language3}}
                <span>|</span>
                <span class="language3">
                    {{prof_language3}}
                </span>
                {{/if}}
            </p>
            <p class="insurance">Insurance Accepted: {{#if prof_insurance1}}
                <span class="insurance1">
                    {{prof_insurance1}}
                </span>
                {{/if}} {{#if prof_insurance2}}
                <span>|</span>
                <span class="insurance2">
                    {{prof_insurance2}}
                </span>
                {{/if}} {{#if prof_insurance3}}
                <span>|</span>
                <span class="insurance3">
                    {{prof_insurance3}}
                </span>
                {{/if}}
            </p>
        </div>
        <div class="col s3">
            <div class="practice-info">Practice Info:
                <p class="street">{{prof_street}}</p>
                <p class="city">{{prof_city}},</p>
                <p class="state">{{prof_state}}</p>
                <p class="zip">{{prof_zip}}</p>
                <p class="email">{{prof_email}}</p>
                <p class="phone">{{prof_phone}}</p>
            </div>
        </div>
    </div>
    <div class="row" id="review-row">
        <div class="collapsible-header">
            <span>Rating: {{averageRating}}</span>
            <span>-</span>
            <a id="reviews">Read Reviews</a>
        </div>
    </div>
    <div class="row collapsible-body">
        <a class="add-review" href="">Add Review</a>
        <div class="row display-none review-form">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s5">
                        <input  id="first-name-input" class="first-name-input" type="text" placeholder="John">
                        <label for="first-name-input">First Name</label>
                    </div>
                    <div class="input-field col s5">
                        <input id="last-name-input" class="last-name-input" type="text" placeholder="Doe">
                        <label for="last-name-input">Last Name</label>
                    </div>
                    <div class="input-field col s2">
                        <input id="rating-input" class="rating-input" type="text" placeholder="5">
                        <label for="rating-input">Rating</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <textarea id="review-input" class="review-input materialize-textarea" placeholder="Enter review body"></textarea>
                        <label for="review-input">Review Body</label>
                    </div>
                </div>
                <button class="submit-review btn waves-effect waves-light blue lighten-2" type="submit" name="action">Submit</button>
            </form>
        </div>
        <ul id="reviews-ul">
